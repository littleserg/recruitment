<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="recruitment">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.versatile.recruitment.war.gwt.client.theme.gray.Gray'/>
    <inherits name='com.versatile.recruitment.impl.gwt.RecruitmentImpl'/>

    <entry-point class='com.versatile.recruitment.war.gwt.client.RecruitmentEntryPoint'/>

    <extend-property name="locale" values="ru"/>
    <set-property name="locale" value="ru"/>

    <!--<set-property name="user.agent" value="safari"/>-->
    <extend-property name="user.agent" values="gecko1_8"/>
    <set-property name="user.agent" value="gecko1_8"/>


    <source path="client"/>
</module>